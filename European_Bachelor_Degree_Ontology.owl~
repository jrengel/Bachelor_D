        <rdfs:comment xml:lang="en">Assignments of a subject. This property is listed in the TEACH ontology documentation, although is not part of the ontology, reference: http://linkedscience.org/teach/ns/</rdfs:comment>
        <rdfs:range rdf:resource="&teach;Assignment"/>
        <rdfs:domain rdf:resource="&teach;Course"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/ebdo#hasAssignmentMaterial -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/ebdo#hasAssignmentMaterial">
        <rdfs:comment xml:lang="en">This property represents that an assignment of a course can have materials, like book references, slides, etc. This property is listed in the TEACH ontology documentation, although is not part of the ontology, reference: http://linkedscience.org/teach/ns/</rdfs:comment>
        <rdfs:domain rdf:resource="&teach;Assignment"/>
        <rdfs:range rdf:resource="&teach;Material"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/ebdo#hasCombinationOfBachelorDegrees -->

    <owl:ObjectProperty rdf:about="http://purl.org/net/ebdo#hasCombinationOfBachelorDegrees">
        <rdfs:comment xml:lang="en">This property represent that a double bachelor degree, is the combination of two bachelor degrees</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/net/ebdo#Double_Bachelor_Degree"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/net/ebdo#Bachelor_Of_Engineering_And_Architecture"/>
                    <rdf:Description rdf:about="http://purl.org/net/ebdo#Bachelor_Of_Health_Science"/>
                    <rdf:Description rdf:about="http://purl.org/net/ebdo#Bachelor_Of_Social_And_Legal_Science"/>
                    <rdf:Description rdf:about="&bowlogna;Bachelor_Of_Arts"/>
                    <rdf:Description rdf:about="&bowlogna;Bachelor_Of_Science"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


